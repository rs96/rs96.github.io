{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      {
        "code": 120,
        "ignoreTemplateLiterals": true
      }
    ],
    "object-literal-sort-keys": false,

    "quotemark": [true, "single", "jsx-double", "avoid-escape"]
  },
  "rulesDirectory": []
}
